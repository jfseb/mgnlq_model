{
  "name": "mgnlq_model",
  "version": "0.0.1",
  "description": "model access from mongoose",
  "main": "js/index.js",
  "typings": "js/index",
  "scripts": {
    "build": "gulp",
    "test": "gulp test",
    "load_data": "node js/makedb",
    "instrument": "jscoverage js/ gen_cov/ --exclude *.map,*.d.ts",
    "covertest": "./node_modules/.bin/nyc --source-map false nodeunit test/model test/match",
    "coverreportplain": "./node_modules/.bin/nyc --source-map false report --reporter=text-lcov ",
    "coverreport": "./node_modules/.bin/nyc --source-map false report --reporter=text-lcov | coveralls",
    "coverage": "nyc --reporter=text-lcov nodeunit test/model test/match | coveralls",
    "coverage2": "node node_modules/nyc/bin/nyc.js --reporter=text-lcov nodeunit -r test/model test/match | coveralls",
    "testcov2": "export FSD_COVERAGE=1 && nodeunit test/model test/match --reporter=lcov",
    "testcovX": "export FSD_COVERAGE=1 && nodeunit test/model test/match --reporter=lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jfseb/mgnlq_model.git"
  },
  "keywords": [
    "typescript",
    "mgnlq"
  ],
  "author": "jfsebpublic@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jfseb/mgnlq_model/issues"
  },
  "homepage": "https://github.com/jfseb/mgnlq_model#readme",
  "devDependencies": {
    "@types/debug": "0.0.29",
    "@types/lodash": "^4.14.61",
    "@types/mongoose": "^4.7.13",
    "@types/node": "^7.0.5",
    "babel-preset-es2015": "^6.22.0",
    "coveralls": "^2.13.1",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-env": "^0.4.0",
    "gulp-eslint": "^3.0.1",
    "gulp-jsdoc3": "^1.0.1",
    "gulp-nodeunit": "^0.1.0",
    "gulp-run": "^1.7.1",
    "gulp-typescript": "^3.1.5",
    "jscoverage": "^0.6.0",
    "mgnlq_testmodel": "^0.0.1",
    "mgnlq_testmodel_replay": "^0.0.1",
    "mongoose_record_replay": "^0.0.3",
    "nyc": "^10.3.2",
    "typescript": "^2.2.1"
  },
  "dependencies": {
    "@types/node": "^7.0.18",
    "abot_stringdist": "^0.1.0",
    "abot_utils": "^0.1.0",
    "debugf": "^0.1.2",
    "jsonschema": "^1.1.1",
    "mongoose": "^4.9.9",
    "mongoose-schema-jsonschema": "^1.1.13"
  }
}
